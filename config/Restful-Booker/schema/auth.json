{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Auth Token",
    "type": "object",
    "properties": {
        "token": {
            "type": "string",
            "pattern": "^[a-z0-9]{15}$"
        }
    },
    "required": [
        "token"
    ],
    "additionalProperties": false
}