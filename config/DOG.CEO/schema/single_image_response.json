{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Single Image",
    "type": "object",
    "properties": {
        "message": {
            "type": "string",
            "format": "uri",
            "pattern": "^(https|http)://"
        },
        "status": {
            "type": "string",
            "enum": ["success","error"]
        }
    },
    "required": [
        "message",
        "status"
    ]
}