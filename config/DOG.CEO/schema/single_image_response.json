{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "message": {
            "type": "string",
            "format": "uri",
            "pattern": "^(https|http)://"
        },
        "status": {
            "type": "string",
            "enum": ["success","error"]
        }
    },
    "required": [
        "message",
        "status"
    ],
    "additionalProperties": {
        "code": {
            "type": "number"
        }
    }
}