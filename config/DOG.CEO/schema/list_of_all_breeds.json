{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
        "message": {
            "type": "object",
            "patternProperties": {
                "[a-z]+": {
                    "type": "array",
                    "items": { "type": "string" }
                }
            }
        },
        "status": {
            "type": "string",
            "enum": ["success","error"]
        }
    },
    "required": [
        "message",
        "status"
    ]
}