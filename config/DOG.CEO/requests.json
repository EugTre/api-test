{
    "GetRandomImage": {
        "request": {
            "method": "GET",
            "path": "breeds/image/random"
        },
        "response": {
            "status_code": 200,
            "schema_name": "single_image_response"
        }
    },
    "GetMultipleRandomImages": {
        "request": {
            "method": "GET",
            "path": "breeds/image/random/{amount}",
            "path_params": {
                "comment": [
                    "This is just an example of params and it's values.",
                    "It still requires user to add params before request."
                ],
                "amount": 2
            }
        },
        "response": {
            "status_code": 200,
            "schema_name": "multiple_images_response"
        }
    },
    "GetRandomImageByBreed": {
        "request": {
            "method": "GET",
            "path": "breed/{breed}/images/random",
            "path_params": {
                "breed": "pug"
            }
        },
        "response": {
            "status_code": 200,
            "schema_name": "single_image_response"
        }
    },
    "GetMultipleRandomImageByBreed": {
        "request": {
            "method": "GET",
            "path": "breed/{breed}/images/random/{amount}",
            "path_params": {
                "breed": "pug",
                "amount": 2
            }
        },
        "response": {
            "status_code": 200,
            "schema_name": "multiple_images_response"
        }
    }
}
